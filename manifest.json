{
	"manifest_version": 2,
	"name": "darkerQuantum",
	"author": "Golga",
	"applications": {
		"gecko": {
			"id": "darkerQuantum@oyana.org",
			"strict_min_version": "54.0a1"
		}
	},
	"description": "Firefox Quantum darker theme for UI & website.\n\r ",
	"homepage_url": "https://github.com/Oyana/darkerQuantum",
	"version": "0.5.1",
	"browser_action": {
		"default_icon": "img/ico/icon-128.png",
		"default_title": "darkerQuantumSettings"
	},
	"icons": {
		"16": "img/ico/icon-16.png",
		"32": "img/ico/icon-32.png",
		"48": "img/ico/icon-48.png",
		"64": "img/ico/icon-64.png",
		"96": "img/ico/icon-96.png",
		"128": "img/ico/icon-128.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"<all_urls>",
		"alarms",
		"activeTab",
		"tabs",
		"storage",
		"theme"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"options_ui": {
		"page": "html/options.html",
		"browser_style": true
	}
}
